<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
  <head>
    <meta charset="utf-8">
    <meta name="generator" content="pandoc">
    <meta name="viewport" content="width=device-width,
                                   initial-scale=1.0,
                                   user-scalable=yes">
    <meta name="author" content="Daniel Jour">
    $for(authors)$
    <meta name="author" content="$authors$">
    $endfor$
    $if(date)$
    <meta name="dcterms.date" content="$date$">
    $endif$
    $if(keywords)$
    <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
    $endif$
    $if(description)$
    <meta name="description" content="$description$">
    $endif$
    <title>$pagetitle$$if(title-suffix)$ – $title-suffix$$endif$</title>
    <script async defer src="$pathToProjectRoot$/js.min.js"></script>
    <style type="text/css">code{white-space: pre;}</style>
    $if(quotes)$
    <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
    $endif$
    <style type="text/css">
      body {
      padding-left: 1em;
      padding-right: 1em;
      margin-left: auto;
      margin-right: auto;
      max-width: 42em;
      font-size: 100%;
      font-family: Verdana, Geneva, sans-serif;
      line-height: 24px;
      }
      div.sourceCode {
      line-height: normal;
      }
      .heading {
          color: #002560;
          display: block;
          font-size: 2em;
          margin-block-start: 0.67em;
          margin-block-end: 0.67em;
          margin-inline-start: 0px;
          margin-inline-end: 0px;
          font-weight: bold;
      }
      a:link, a:visited {
      color: #008aa3;
      text-decoration: none;
      }
      a:hover {
      color: #004d60;
      text-decoration: none;
      }
      a:active {
      color: white;
      background-color: #008aa3;
      }
      footer {
      font-size: 75%;
      }
      .not-my-info {
      font-size: 0;
      }
      pre > code {
      overflow-x: auto;
      display: block;
      }
    </style>
    $if(highlighting-css)$
    <style type="text/css">
      $highlighting-css$
    </style>
    $endif$
    $if(math)$
    $math$
    $endif$
    <!--[if lt IE 9]>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv-printshiv.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <header>
      <p class="heading"><a href="$pathToProjectRoot$/index.html">musteresel's blog</a></h1>
    </header>
    <hr>
    <article>
      $if(title)$
      <header>
        <h1 class="title">$title$</h1>
        $if(subtitle)$
        <p class="subtitle">$subtitle$</p>
        $endif$
        $for(authors)$
        <p class="author">$authors$</p>
        $endfor$
        $if(date)$
        <p class="date">$date$</p>
        $endif$
        $if(tags)$
        <p class="tags">tagged:
          $for(tags)$
          <a href="$pathToProjectRoot$/posts/tagged/$tags$/index.html">$tags$</a>$sep$,
          $endfor$</p>
        $endif$
      </header>
      $endif$
      $body$
      $for(include-after)$
      $include-after$
      $endfor$
      $if(postref)$<hr>
      <footer>
        <a href="https://docs.google.com/forms/d/e/1FAIpQLSfV7TfT4OIcpopar3hBoLnzHBmbOi85ysjX23cQsvKzBCy4Dw/viewform?usp=pp_url&entry.1949939362=$postref$">
          Send me a message regarding this post</a>
      </footer>
      $endif$
    </article>
    <hr>
    <footer id=>
      <a href="$pathToProjectRoot$/legal.html">Legal Notice / Impressum</a>
      &nbsp;&nbsp;
      <a href="$pathToProjectRoot$/privacy.html">Privacy / Datenschutz</a>
    </footer>
  </body>
</html>
